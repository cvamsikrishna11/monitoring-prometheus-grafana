global:
  scrape_interval: 15s
  external_labels:
    monitor: 'prometheus'

scrape_configs:
  - job_name: 'prometheus'
    static_configs:
    - targets: ['localhost:9090']
  - job_name: 'node'
    ec2_sd_configs:
      - region: us-east-1        
        port: 9100
  - job_name: 'jenkins'
    metrics_path: /prometheus/
    ec2_sd_configs:     
      - region: us-east-1        
        port: 8080
        filters:
          - name: tag:Application
            values:
              - jenkins
  
  - job_name: 'node_exporter'
    static_configs:
      - targets: ['18.219.214.162:9100']  # static
      - targets: ['7.4.5.6:9100'] #static

  - job_name: 'pushgateway'
    static_configs:
      - targets: ['localhost:9091']